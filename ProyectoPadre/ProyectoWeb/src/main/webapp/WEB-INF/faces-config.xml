<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">
	<application>

		<resource-bundle>
			<base-name>resources.application</base-name>
			<var>mensajes</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>productoBean</managed-bean-name>
		<managed-bean-class>co.uniquindio.beans.ProductoBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>seguridadBean</managed-bean-name>
		<managed-bean-class>co.uniquindio.beans.SeguridadBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>